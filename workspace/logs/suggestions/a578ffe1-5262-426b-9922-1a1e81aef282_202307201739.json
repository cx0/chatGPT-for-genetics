query searchFasudil {
  search(queryString: "fasudil", entityNames: "target") {
    total
    hits {
      id
      name
      entity
      object {
        ... on Target {
          associatedDiseases (orderByScore: "score", page: { size: 10, index: 0 }) {
            rows {
              score
              disease {name}
            }
          }
        }
      }
    }
  }
}