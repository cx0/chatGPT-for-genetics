{
  "AWSEBDockerrunVersion": 1,
  "containerDefinitions": [
    {
      "name": "nextjs-app",
      "image": "foadgreen/chatgpt-for-genetics-nextjs-app:latest",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 3000
        }
      ]
    },
    {
      "name": "flask-app",
      "image": "foadgreen/chatgpt-for-genetics-flask-app:latest",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 5001,
          "containerPort": 5001
        }
      ]
    }
  ]
}
